<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/titleLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/usvisa" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/report"
                    android:textSize="32sp"
                    android:theme="@style/textViewStyleTitle" />


            </RelativeLayout>

            <android.support.v7.widget.CardView
                android:id="@+id/cardviewAge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleLayout"
                android:onClick="ageClick"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:id="@+id/rel1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageAge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/age" />

                    <TextView
                        android:id="@+id/text1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageAge"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Age"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewAgePoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>



            <android.support.v7.widget.CardView
                android:id="@+id/cardviewEducation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="educationClick"
                android:layout_below="@+id/cardviewAge"
                android:layout_margin="12dp">

                <RelativeLayout
                    android:id="@+id/layou"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageEducation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/education" />

                    <TextView
                        android:id="@+id/text2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="8dp"
                        android:layout_toRightOf="@+id/imageEducation"
                        android:text="Education"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                    <TextView
                        android:id="@+id/textViewEducationPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewEnglish"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="englishClick"
                android:layout_below="@+id/cardviewEducation"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageEnglish"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/english" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageEnglish"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="English"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewEnglishPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewJob"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="jobClick"
                android:layout_below="@+id/cardviewEnglish"
                android:layout_margin="12dp">
                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageJob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/job" />

                    <TextView
                        android:id="@+id/text4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageJob"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Job"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewJobPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewNobel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="nobelClick"
                android:layout_below="@+id/cardviewJob"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:id="@+id/rel5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageNobel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/nobel" />

                    <TextView
                        android:id="@+id/text5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageNobel"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Nobel Prize"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewNobelPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewOlympics"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="olympicsClick"
                android:layout_below="@+id/cardviewNobel"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:id="@+id/rel6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageOlympics"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/olympics" />

                    <TextView
                        android:id="@+id/text6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageOlympics"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Olympics"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewOlympicsPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewInvestor"
                android:layout_width="match_parent"
                android:onClick="investorClick"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardviewOlympics"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:id="@+id/rel7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageInvestor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/invesment" />

                    <TextView
                        android:id="@+id/text7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageInvestor"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Investor"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewInestorPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cardviewSpouses"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="spousesClick"
                android:layout_below="@+id/cardviewInvestor"
                android:layout_margin="12dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageSpouses"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:layout_centerVertical="true"
                        android:src="@drawable/spouses" />

                    <TextView
                        android:id="@+id/text8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/imageSpouses"
                        android:layout_marginLeft="8dp"
                        android:layout_centerVertical="true"
                        android:text="Spouses"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />
                    <TextView
                        android:id="@+id/textViewSpousesPoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="10"
                        android:textSize="32sp"
                        android:theme="@style/textViewStyleEducation" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <TextView
                android:id="@+id/tvTotalPoint"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardviewSpouses"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:gravity="center_horizontal"
                android:text="10"
                android:textSize="32sp"
                android:theme="@style/textViewStylePoint" />

            <TextView
                android:id="@+id/tvTotalPointMessage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvTotalPoint"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="4dp"
                android:gravity="center_horizontal"
                android:textSize="24sp"
                android:theme="@style/textViewStyleMessage" />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/tvTotalPointMessage"
                android:onClick="reportPreviousClick"
                android:text="@string/previous"
                android:textSize="24sp"
                android:textStyle="bold"
                android:theme="@style/PrimaryFlatButton" />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvTotalPointMessage"
                android:layout_alignParentRight="true"
                android:onClick="refrenceClick"
                android:text="@string/refrences"
                android:textSize="24sp"
                android:textStyle="bold"
                android:theme="@style/PrimaryFlatButton" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@color/textColor"></LinearLayout>
</RelativeLayout>